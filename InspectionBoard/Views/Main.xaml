<Window x:Class="InspectionBoard.Views.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InspectionBoard.Views"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="Main" Height="450" Width="800">
    <Grid>
        <Button Content="click" Command="{Binding NavigateCommand}" CommandParameter="Auth"/>
        <TextBlock VerticalAlignment="Bottom" Text="{Binding Message}" HorizontalAlignment="Center" FontSize="50"/>
        <ContentControl prism:RegionManager.RegionName="ContentRegion" Margin="5"  />
    </Grid>
    
</Window>
